{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://bnsoluct.onrender.com/api/$1" },
    { "source": "/chat", "destination": "https://bnsoluct.onrender.com/chat" }
  ]
}
